{
  "name": "codexten/yii-module-file",
  "type": "library",
  "description": "file  module for yii 2.0 or higher",
  "require": {
    "codexten/yii-web": "~2.0.0",
    "alexantr/yii2-elfinder": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "codexten\\yii\\modules\\file\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    },
    "config-plugin": {
      "core": "config/core.php",
      "moduleFile": "config/moduleFile.php"
    }
  }
}
